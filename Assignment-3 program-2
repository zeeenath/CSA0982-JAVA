package water;
public class River_Water
{
protected void saltLevel()

{
System.out.println("Salt level is above 20");
}
protected void fishType()
{
System.out.println("jelly fish lives here");
}
public static void main(String ar[])
{
River_Water rw=new River_Water();
rw.saltLevel();
rw.fishType();
}

}
