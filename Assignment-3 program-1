import water.*;
class Mixed extends Sea_Water
{
public static void main(String ar[])
{
Sea_Water sw=new Sea_Water();
sw.saltLevel();
sw.fishType();
}
}

package water;
public class Lake_Water
{
protected void saltLevel()

{
System.out.println("Salt level is above 30");
}
protected void fishType()
{
System.out.println("gold fish lives here");
}
public static void main(String ar[])
{
Lake_Water lw=new Lake_Water();
lw.saltLevel();
lw.fishType();
}

}
